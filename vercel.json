{
    "version": 2,
    "builds": [
        {
            "src": "/public/index.php",
            "use": "@vercel-community/php"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|storage)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}